{
	"name": "@ctx-core/iex-graphql",
	"version": "9.0.98",
	"description": "@ctx-core/iex-graphql",
	"keywords": [
		"ctx-core",
		"iex",
		"graphql",
		"apollo"
	],
	"homepage": "https://github.com/ctx-core/iex-graphql#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/iex-graphql/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/iex-graphql/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"module": "./dist/index.js",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/graphql": "workspace:^9.0.40",
		"@ctx-core/iex": "workspace:^5.0.93",
		"graphql-tag": "^2.12.6"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.52",
		"@swc/core": "^1.2.118",
		"graphql": "^16.1.0",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
