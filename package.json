{
	"name": "@ctx-core/iex-graphql",
	"version": "9.0.103",
	"description": "@ctx-core/iex-graphql",
	"keywords": [
		"ctx-core",
		"iex",
		"graphql",
		"apollo"
	],
	"homepage": "https://github.com/ctx-core/iex-graphql#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/iex-graphql/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/iex-graphql.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./src/index.ts"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/graphql": "workspace:^9.0.43",
		"@ctx-core/iex": "workspace:^5.0.99",
		"graphql-tag": "^2.12.6"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.120",
		"graphql": "^16.1.0",
		"rimraf": "^3.0.2",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "./dist/index.js"
}